{
  "version": "0.0.6",
  "name": "cca-js",
  "main": "lib/index.js",
  "private": true,
  "typescript": {
    "definition": "build/index"
  },
  "typings": "build/index",
  "scripts": {
    "vscode-install": "npm install",
    "clean": "rimraf build",
    "precompile": "npm run clean",
    "compile": "tsc",
    "postcompile": "echo compile done",
    "prebuild": "npm run compile",
    "build": "npm version patch",
    "prebuild-web": "rimraf build/web",
    "build-web": "copyfiles -f ./assets/* build/cca-lib.js build/cca-web.js build/*Lib.js build/*Web.js ./build/web",
    "postbuild-web": "ls build/web/*",
    "postbuild": "echo build done",
    "pretest": "npm run compile",
    "test": "node ./build/roomKeyNode.js"
  },
  "devDependencies": {
    "copyfiles": "^1.2.0",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.2",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "prompt": "^1.0.0"
  }
}
